{
   

   "author": "Omnivore Media, Inc",
   "background": {
      "page": "/views/background.html",
      "persistent": true
   },
   "browser_action": {
      "default_icon": {
         "16": "/images/extension/icon-16.png",
         "19": "/images/extension/icon-19.png",
         "24": "/images/extension/icon-24.png",
         "32": "/images/extension/icon-32.png",
         "38": "/images/extension/icon-38.png",
         "48": "/images/extension/icon-48.png"
      },
      "default_title": "Omnivore Save Article"
   },
   "browser_specific_settings": {
      "gecko": {
         "id": "save-extension@omnivore.app"
      }
   },
   "commands": {
      "_execute_browser_action": {
         "description": "Save the current tab to Omnivore",
         "suggested_key": {
            "default": "Alt + O"
         }
      }
   },
   "content_scripts": [ {
      "js": [ "/scripts/common.js", "/scripts/content/toast.js", "/scripts/content/page-info.js", "/scripts/content/prepare-content.js", "/scripts/content/content-listener-script.js" ],
      "matches": [ "https://*/**", "http://*/**" ]
   }, {
      "all_frames": true,
      "js": [ "/scripts/content/grab-iframe-content.js" ],
      "matches": [ "https://*/**", "http://*/**" ]
   } ],
   "content_security_policy": "default-src 'none'; child-src 'none'; manifest-src 'none'; media-src 'none'; object-src 'none'; worker-src 'none'; connect-src https://storage.googleapis.com/ https://api-prod.omnivore.app/api/ blob:; frame-src 'none'; font-src 'none'; img-src data:; script-src 'self'; script-src-elem 'self'; script-src-attr 'none'; style-src 'self'; style-src-elem 'self'; style-src-attr 'none'; base-uri 'none'; form-action 'none'; block-all-mixed-content; upgrade-insecure-requests; report-uri https://api.jeurissen.co/reports/csp/webext/omnivore/",
   "default_locale": "en",
   "description": "Save PDFs and Articles to your Omnivore library",
   "developer": {
      "name": "Omnivore Media, Inc",
      "url": "https://omnivore.app/"
   },
   "homepage_url": "https://omnivore.app/",
   "icons": {
      "128": "/images/extension/icon-128.png",
      "16": "/images/extension/icon-16.png",
      "24": "/images/extension/icon-24.png",
      "256": "/images/extension/icon-256.png",
      "32": "/images/extension/icon-32.png",
      "48": "/images/extension/icon-48.png",
      "96": "/images/extension/icon-96.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "21",
   "minimum_opera_version": "15",
   "name": "Omnivore",
   "options_ui": {
      "open_in_tab": true,
      "page": "/views/options.html"
   },
   "permissions": [ "activeTab", "storage", "contextMenus", "https://*/**", "http://*/**" ],
   "short_name": "Omnivore",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.8.0",
   "web_accessible_resources": [ "views/toast.html" ]
}
